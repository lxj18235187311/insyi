<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.insyi.core.dao.TestTbDao">
	<insert id="insertTestTb" parameterType="testTb">
		insert into test_td
		(id,name,birthday)
		values
		(#{id},#{name},#{birthday})
	</insert>
</mapper>